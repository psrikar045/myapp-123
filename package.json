{
  "name": "myapp-123",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test --grep @visual",
    "test:responsive": "playwright test --grep @responsive",
    "test:codegen": "playwright codegen http://localhost:4200",
    "test:all": "npm run test:jest:coverage && npm run test:e2e",
    "test:report": "allure generate allure-results --clean && allure open",
    "serve:ssr:myapp-123": "node dist/myapp-123/server/server.mjs",
    "lint": "ng lint"
  },
  "private": true,
